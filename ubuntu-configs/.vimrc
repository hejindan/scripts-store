set fenc=utf-8 
set fencs=utf-8,usc-bom,euc-jp,gb18030,gbk,gb2312,cp936 
set nocompatible
set history=100
set nu
filetype on
filetype plugin on
filetype indent on
set iskeyword+=_,$,@,%,#,-
syntax on
set cmdheight=2
set showmatch
set matchtime=5
set ignorecase
set autoindent
set tabstop=4
set softtabstop=4
set shiftwidth=4
set clipboard+=unnamed
set selectmode=mouse,key
